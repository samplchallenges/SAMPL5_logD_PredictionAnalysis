file = {{F548BF5A-277B-481A-86E5-D1AD41C4FF11.pdf:/Users/bannanc/Documents/Library.papers3/Files/F5/F548BF5A-277B-481A-86E5-D1AD41C4FF11.pdf:application/pdf;F548BF5A-277B-481A-86E5-D1AD41C4FF11.pdf:/Users/bannanc/Documents/Library.papers3/Files/F5/F548BF5A-277B-481A-86E5-D1AD41C4FF11.pdf:application/pdf}},
