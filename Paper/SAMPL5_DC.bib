pages = {1192--1201},
month = jun,
affiliation = {{\dag}Department of Chemistry, Zhejiang University, Hangzhou 310027, China.},
keywords = {logP Paper, SAMPL5},
doi = {10.1021/acs.jcim.5b00106},
pmid = {26010106},
language = {English},
read = {Yes},
